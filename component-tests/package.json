{
  "name": "component-tests",
  "version": "1.0.0",
  "description": "Cucumber Component Tests for Personal Data service",
  "scripts": {
    "lint": "./node_modules/.bin/eslint \"./**/*.{ts, js}\"",
    "lint-fix": "./node_modules/.bin/eslint --fix \"./**/*.{ts, js}\"",
    "format": "./node_modules/.bin/prettier --check \"./**/*.{ts, js}\"",
    "format-fix": "./node_modules/.bin/prettier --write \"./**/*.{ts, js}\"",
    "test": "rm -f ./report/component-test* && ./node_modules/.bin/cucumber-js --exit -f json:report/component-tests-report.json  ; COMPONENT_TEST_RESULT=$?; node index.js ; ./node_modules/.bin/cross-cat report/component-tests-report.json | ./node_modules/.bin/cucumber-junit > report/component-tests-report.xml ; exit $COMPONENT_TEST_RESULT",
    "test-tag": "./node_modules/.bin/cucumber-js -p default --tags"
  },
  "dependencies": {
    "@supercharge/promise-pool": "1.8.0",
    "@types/amqplib": "0.8.2",
    "@types/axios": "0.14.0",
    "@types/chai": "4.2.22",
    "@types/chai-like": "1.1.1",
    "@types/chai-things": "0.0.35",
    "@types/cucumber": "6.0.1",
    "@types/deep-equal-in-any-order": "1.0.1",
    "@types/jsonwebtoken": "8.5.5",
    "@types/node": "13.13.36",
    "@types/pg": "7.14.7",
    "amqp-ts": "1.8.0",
    "axios": "0.21.4",
    "axios-debug-log": "0.8.4",
    "axios-logger": "2.5.0",
    "chai": "4.3.4",
    "chai-like": "1.1.1",
    "chai-things": "0.2.0",
    "cucumber": "5.1.0",
    "cucumber-html-reporter": "5.5.0",
    "cucumber-pretty": "1.5.2",
    "cucumber-tsflow": "3.4.1",
    "deep-equal-in-any-order": "1.1.15",
    "jsonwebtoken": "8.5.1",
    "pg": "8.5.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "uuid-parse": "1.1.0"
  },
  "devDependencies": {
    "@types/uuid-parse": "1.0.0",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "cross-cat": "1.0.0",
    "cucumber-html-reporter": "5.5.0",
    "cucumber-junit": "1.7.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-prettier": "3.4.1",
    "mem": "6.1.0",
    "prettier": "2.0.5",
    "winston": "3.3.3"
  }
